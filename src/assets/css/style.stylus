// 提供需求完成之后跳转的页面的样式

.journey
  // background #eee
  // z-index: -9999
  //左边箭头的样式
  .van-nav-bar
    border-bottom 1px solid #ccc
    // margin-bottom 20px
  .van-nav-bar .van-icon
    font-size  20px
    color #000



  //需求行程的图片和加号
  .journey-main
    background #eee
    width 100%
    position relative
    // 需求提交成功
    .journey-top
      width 92%
      border 15px solid #f7f7f7
      position absolute
      top 27%
      left 50%
      transform translateX(-50%)
      box-shadow 1px 3px 2px #eee
      padding-bottom: 10%
      .journey-second
        padding 20px 0 0 0
        .custom-image
          width 100%
          height 80%
          padding 0
          img
            width 100%
            height 60%
        p
          margin-bottom 15px
          font-size 18px
        .van-icon
          width 65px
          height 65px
          margin-top 20px
          font-size 65px
          // border 1px solid #000
          z-index 999
  // 完善需求
  .journey-mid
    position absolute
    top 35%
    left 0
    font-size 17px
    background #f7f7f7
    width 100%
  // 酒店机票选择
  .journey-end
    width 92%
    border 15px solid #f7f7f7
    // box-shadow 1px 3px 2px #eee
    position absolute
    top 41.5%
    left 0%
    font-size 15px
    border-bottom none
    .textneed
      padding-bottom 15px
      border-bottom 74px solid #f7f7f7
    .textneed .text
      width 92%
      padding-top 10px
      padding-left 10px
      font-size 15px
      background #f7f7f7
      // border-radius 10px solid red
      border none
      border 1px solid #ccc
  //底部确定按钮
  .confirm
    position fixed
    bottom 0
    left 50%
    transform translateX(-50%)
    width 100%
    height 60px
    background #27d461
    line-height 50px
    color #fff
    font-size 20px
  .van-nav-bar__text
    color #27d461
  .van-nav-bar__title
    font-size 17px

